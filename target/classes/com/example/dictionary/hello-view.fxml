<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<TabPane prefHeight="419.0" prefWidth="814.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.dictionary.HelloController">
    <tabs>
        <Tab closable="false" text="Search word">
            <content>
                <VBox alignment="CENTER" prefHeight="461.0" prefWidth="814.0" spacing="20.0">
                    <padding>
                        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
                    </padding>

                    <Label fx:id="welcomeText" />
                    <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Distributed Systems Project one" wrappingWidth="514.13671875">
                        <font>
                            <Font size="32.0" />
                        </font>
                    </Text>
                    <TextField fx:id="search_text_box" />
                    <Button fx:id="search_btn" mnemonicParsing="false" onAction="#onSearchBtnClicked" text="Search Word" />
                    <TableView fx:id="search_result_table" prefHeight="208.0" prefWidth="819.0">
                        <columns>
                            <TableColumn prefWidth="75.0" text="index" />
                            <TableColumn prefWidth="206.0" text="word" />
                            <TableColumn prefWidth="537.0" text="meaning" />
                        </columns>
                    </TableView>
                </VBox>
            </content>
        </Tab>
        <Tab closable="false" text="Add Word">
            <content>
                <VBox alignment="CENTER" prefHeight="402.0" prefWidth="814.0" spacing="20.0">
                    <padding>
                        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
                    </padding>
                    <children>
                        <Label fx:id="welcomeText1" />
                        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Add Word to your dictionary">
                            <font>
                                <Font size="36.0" />
                            </font>
                        </Text>
                        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Word" wrappingWidth="767.3046875">
                            <font>
                                <Font size="16.0" />
                            </font>
                        </Text>
                        <TextField fx:id="add_word" />
                        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Definition" wrappingWidth="772.125">
                            <font>
                                <Font size="16.0" />
                            </font>
                        </Text>
                        <TextArea fx:id="add_word_def" prefHeight="45.0" prefWidth="774.0" />
                        <Button fx:id="add_word_to_dictionary" mnemonicParsing="false" onAction="#onAddWordBtnClicked" text="Add word to dictionary">
                            <font>
                                <Font size="18.0" />
                            </font>
                        </Button>
                  <Text fx:id="add_message_text_view" strokeType="OUTSIDE" strokeWidth="0.0" />
                    </children>
                </VBox>
            </content>
        </Tab>
        <Tab closable="false" text="All words">
            <content>
                <VBox alignment="CENTER" prefHeight="461.0" prefWidth="814.0" spacing="20.0">
                    <padding>
                        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
                    </padding>
                    <children>
                        <Label fx:id="welcomeText11" />
                        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="My Dictionary" wrappingWidth="209.13671875">
                            <font>
                                <Font size="32.0" />
                            </font>
                        </Text>
                        <TableView fx:id="all_words_table" prefHeight="323.0" prefWidth="774.0">
                            <columns>
                                <TableColumn prefWidth="75.0" text="index" />
                                <TableColumn prefWidth="206.0" text="word" />
                                <TableColumn prefWidth="537.0" text="meaning" />
                            </columns>
                        </TableView>
                    </children>
                </VBox>
            </content>
        </Tab>
    </tabs>
</TabPane>
